<div class="relative bg-white">
  <div class="flex flex-col w-screen">
    <div class="flex items-start justify-start bg-gray-100">
      <div
        class="inline-flex flex-col space-y-5 items-start justify-start w-64 pt-5 pb-4 bg-gray-900 "
      >
        <div class="flex flex-col items-start justify-start w-full px-4">
          <div class="inline-flex items-center justify-start w-36 flex-1 pr-5">
            <img
              class="flex-1 h-full rounded-lg"
              src="<%= Routes.static_path(@conn, "/images/fanclub_white_logotype_horizontal.png") %>"
            />
          </div>
        </div>
        <div
          class="flex flex-col space-y-1 items-start justify-start w-full flex-1 px-2"
        >
          <div
            class="inline-flex items-center justify-start w-full py-2 pl-2 pr-3 bg-black rounded-md"
          >
            <div class="flex space-x-3 items-center justify-start">
              <img
                class="w-1/3 h-full rounded-lg"
                src="<%= Routes.static_path(@conn, "/images/home.svg") %>"
              />
              <p class="text-sm font-semibold leading-tight text-white">
                Dashboard
              </p>
            </div>
          </div>
          <div
            class="inline-flex items-center justify-start w-full py-2 pl-2 pr-3 rounded-md"
          >
            <div class="flex space-x-3 items-center justify-start">
              <img
                class="w-1/3 h-full rounded-lg"
                src="<%= Routes.static_path(@conn, "/images/users.svg") %>"
              />
              <p class="text-sm font-semibold leading-tight text-gray-300">
                Team
              </p>
            </div>
          </div>
          <div
            class="inline-flex items-center justify-start w-full py-2 pl-2 pr-3 rounded-md"
          >
            <div class="flex space-x-3 items-center justify-start">
              <img
                class="w-6 h-full rounded-lg"
                src="<%= Routes.static_path(@conn, "/images/team.svg") %>"
              />
              <p class="text-sm font-semibold leading-tight text-gray-300">
                Projects
              </p>
            </div>
          </div>
        </div>
      </div>
      <div
        class="inline-flex flex-col items-start justify-start w-full h-screen"
      >
        <div
          class="inline-flex h-14 w-full space-x-6 items-center justify-start px-4 bg-white shadow"
        >
          <div
            class="flex space-x-3 items-center justify-start py-2 pl-0.5 pr-3 "
          >
            <img
              class="w-1/3 ml-1"
              src="<%= Routes.static_path(@conn, "/images/search.svg") %>"
            />
            <p class="text-sm leading-tight text-gray-600">Search</p>
          </div>
        </div>
        <div class="flex flex-col w-full items-start justify-start py-6">
          <div class="inline-flex items-center justify-start px-8">
            <p class="text-2xl font-semibold leading-loose text-black">
              Dashboard
            </p>
          </div>
          <div
            class="inline-flex flex-col h-4/6 w-11/12 ml-8 items-start justify-start bg-white shadow border rounded-lg border-gray-200"
          ></div>
        </div>
      </div>
    </div>
    <div class="flex items-start justify-start bg-black h-16"></div>
  </div>
</div>
